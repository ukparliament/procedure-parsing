<article>
	<p>
    <%- if business_item.date %>

      <time><%= business_item.date %></time>
    <% end %>


<a href="<%= business_item.web_link %>" title="<%= business_item.web_link %>">
  <%= business_item.web_link_domain %></a>

    <% business_item.steps.each do | step | %>
    <p>&mdash; <%= step.name %><br>
      <small><%= step.house_label %></small></p>

  <% end %>

	</p>

</article>

<hr>