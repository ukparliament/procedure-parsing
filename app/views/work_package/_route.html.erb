<tr>
	<%= content_tag( 'td', "#{route[0].id} (#{route[0].triple_store_id})" ) %>
	<%= content_tag( 'td', route[1][:parsed] ) %>
	<%= content_tag( 'td', route[1][:status] ) %>
	<%= content_tag( 'td', route[1][:current] ) %>
	<td>
		<% if route[1][:source_step_type] == 'Business step' %>
			<%= route[1][:source_step_name] %>
		<% else -%>
			<%= route[1][:source_step_name] %> <%= route[1][:source_step_type] %>
		<% end -%>
	</td>
	<td>
		<% if route[1][:target_step_type] == 'Business step' %>
			<%= route[1][:target_step_name] %>
		<% else -%>
			<%= route[1][:target_step_name] %> <%= route[1][:target_step_type] %>
		<% end -%>
	</td>
</tr>