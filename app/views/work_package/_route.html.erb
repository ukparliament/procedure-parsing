<tr>
	<%= content_tag( 'td', route[0] ) %>
	<td>
		<% if route[1][:route].source_step_type == 'Business step' %>
			<%= route[1][:route].source_step_name %>
		<% else -%>
			<%= route[1][:route].source_step_name %> <%= route[1][:route].source_step_type %>
		<% end -%>
	</td>
	<td>
		<% if route[1][:route].target_step_type == 'Business step' %>
			<%= route[1][:route].target_step_name %>
		<% else -%>
			<%= route[1][:route].target_step_name %> <%= route[1][:route].target_step_type %>
		<% end -%>
	</td>
	<%= content_tag( 'td', route[1][:current] ) %>
	<%= content_tag( 'td', route[1][:parse_pass_count] ) %>
	<%= content_tag( 'td', route[1][:parsed] ) %>
	<%= content_tag( 'td', route[1][:status] ) %>
</tr>