<tr>
	<%= content_tag( 'td', link_to( work_package.id, "https://procedureeditor.azurewebsites.net/WorkPackage/#{work_package.id}" ) ) %>
	<%= content_tag( 'td', work_package.triplestore_id ) %>
	<% if work_package.has_web_link? -%>
		<%= content_tag( 'td', link_to( work_package.has_web_link?, work_package.web_link ) ) %>
		<%= content_tag( 'td', work_package.web_link_http_count_correct? ) %>
		<%= content_tag( 'td', work_package.web_link_response_code ) %>
	<% else -%>
		<%= content_tag( 'td', work_package.has_web_link? ) %>
		<%= content_tag( 'td', 'N/A' ) %>
		<%= content_tag( 'td', 'N/A' ) %>
	<% end -%>
</tr>